    [[?]]

    <section class="grey section">
      <div class="container">
        <div class="row">
          <div id="content" class="col-md-12 col-sm-12 col-xs-12">
            [[~ model.feed:record ]]
              <div class="blog-wrapper">
                <div class="blog-title">
                  <a class="category_title" href="/category/[[= record.categoryId ]]">[[= record.category]]</a>
                  <h2><a href="/post/[[= record.id]]">[[= record.title]]</a></h2>
                </div>
                [[? record.imageUrl ]]
                  <div class="blog-image">
                    <a href="/post/[[= record.id]]">
                      <img src="[[= record.imageUrl]]" alt="[[= record.title]]">
                    </a>
                  </div>
                [[?]]
                <div class="blog-desc">
                  <p>[[= record.shortcut]]</p>
                  <a href="/post/[[= record.id]]" class="readmore">Read More</a>
                </div>
              </div>
            [[~]]

            [[? model.pages > 1 ]]
              <hr class="invis">
              <nav class="text-center">
                <ul class="pagination">
                  [[? model.currentPage !== 1 ]]
                    <li>
                      <a href="/?page=1" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                      </a>
                    </li>
                  [[?]]
                  [[ for (var i = 1; i <= model.pages; i++) { ]]
                    <li><a href="/?page=[[= i]]" [[? i === model.currentPage ]]class="active"[[?]]>[[= i ]]</a></li>
                  [[ } ]]
                  [[? model.currentPage !== model.pages ]]
                    <li>
                      <a href="/?page=[[= model.pages ]]" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                      </a>
                    </li>
                  [[?]]
                </ul>
              </nav>
            [[?]]
          </div>
        </div>
      </div>
